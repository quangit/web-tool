---
import FileHashPage from '../../components/FileHashPage.astro';
const title = "MD4 File Hash";
const description = "This MD4 online tool helps you calculate the hash of a file from local or URL using MD4 without uploading the file. It also supports HMAC.";
const algorithmName = "MD4";
const jsFile = "md4.min.js";
const activePage = "hash/md4_file_hash";
---
<FileHashPage 
    title={title}
    description={description}
    algorithmName={algorithmName}
    jsFile={jsFile}
    activePage={activePage}
/>
